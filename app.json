{
  "expo": {
    "name": "cairo-mobile",
    "slug": "cairo-mobile",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icons/kkrt-logo.png",
    "scheme": "cairomobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.kakarot.cairo-mobile",
      "appleTeamId": "3767P43YCA",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSSupportsOpeningDocumentsInPlace": true,
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeName": "Cairo Proof File",
            "CFBundleTypeRole": "Editor",
            "LSItemContentTypes": ["public.json"],
            "LSHandlerRank": "Alternate",
            "CFBundleTypeExtensions": ["cairoproof.json", "json"],
            "CFBundleTypeDescription": "Cairo ZK Proof File"
          }
        ],
        "UTExportedTypeDeclarations": [
          {
            "UTTypeIdentifier": "org.kakarot.cairo-proof",
            "UTTypeDescription": "Cairo ZK Proof File",
            "UTTypeConformsTo": ["public.json"],
            "UTTypeTagSpecification": {
              "public.filename-extension": ["cairoproof.json", "json"],
              "public.mime-type": ["application/json"]
            }
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-kkrt-logo.png",
        "monochromeImage": "./assets/icons/adaptive-kkrt-logo.png",
        "backgroundColor": "#003D29"
      },
      "edgeToEdgeEnabled": true,
      "package": "org.kakarot.cairo_mobile",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "file",
              "mimeType": "application/json"
            },
            {
              "scheme": "content",
              "mimeType": "application/json"
            }
          ],
          "category": ["DEFAULT"]
        },
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "file",
              "pathPattern": ".*\\.cairoproof\\.json"
            },
            {
              "scheme": "content",
              "pathPattern": ".*\\.cairoproof\\.json"
            }
          ],
          "category": ["DEFAULT"]
        }
      ]
    },
    "extra": {
      "eas": {
        "projectId": "b06a7ffa-d764-4c04-abf2-ffb7999164ae"
      }
    },
    "owner": "kkrt-labs",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "extraProguardRules": "-keep class com.sun.jna.** { *; }\n-keep class * extends com.sun.jna.** { *; }\n-dontwarn java.awt.**\n-dontwarn javax.swing.**\n-keep class expo.modules.cairombindings.** { *; }\n-keep class uniffi.cairo_m_bindings.** { *; }\n-keep class * extends expo.modules.kotlin.records.Record { *; }\n-keepclassmembers class * extends expo.modules.kotlin.records.Record { *; }"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-kkrt-logo.png",
          "backgroundColor": "#003D29",
          "resizeMode": "contain",
          "imageWidth": 200
        }
      ]
    ]
  }
}
